{

  "manifest_version": 2,
  "name": "MUB2T",
  "version": "0.1",

  "description": "A assistant tool for MU B2T",

  "permissions": [
    "*://tms-2.90sky.com/*",
    "webRequest",
    "storage",
    "tabs",
    "browsingData",
    "<all_urls>"
  ],


  "content_scripts": [
    {
      "matches": ["*://travel.ceair.com/*"],
      "js": ["jquery-3.3.1.min.js", "content/content_page.js"]
    }
  ],

  "background": {
    "scripts": ["jquery-3.3.1.min.js", "background.js"]
  },

  "browser_action": {
    "browser_style": true,
    "default_popup": "popup/choose_page.html",
    "default_icon": {
      "40": "icons/zy-40.png"
    }
  }


